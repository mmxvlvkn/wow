                                        <div class="product-settings__checkbox-container">
                                            <input type="checkbox" id="additional_options_4" name="s500000_gold" data-checkbox-name="additional_options" class="product-settings__checkbox">
                                            <label for="additional_options_4" class="product-settings__checkbox-label"><span class="en">500,000 gold</span><span class="ru">500,000 золота
                                                
                                            </span></label>
