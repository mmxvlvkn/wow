                                        <div class="product-settings__range-container">
                                            <input type="range" name="levels" min="65" max="100" value="60" step="1" class="product-settings__range product-settings__range-min">
                                            <input type="range" name="levels" min="65" max="100" value="75" step="1" class="product-settings__range product-settings__range-max">
                                        </div>
                                    </div>
                                </div>
